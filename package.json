{
    "name": "haskell-symbols",
    "displayName": "haskell-symbols",
    "description": "VSCode Haskell symbols search methods",
    "version": "0.1.5",
    "publisher": "miguel-savignano",
    "author": "Hal Henke <halhenke@gmail.com>",
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "images/icon.png",
    "keywords": [
        "haskell",
        "haskell symbols",
        "search code"
    ],
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
